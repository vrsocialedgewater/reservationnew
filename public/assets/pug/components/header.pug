// Page Header Start
.page-header
  .header-wrapper.row.m-0
    .header-logo-wrapper.col-auto.p-0
      .logo-wrapper
        a(href='index.html') 
          img.img-fluid.for-light(src='../assets/images/logo/logo.png', alt='')
          img.img-fluid.for-dark(src='../assets/images/logo/logo_dark.png', alt='')
      .toggle-sidebar
        svg.sidebar-toggle 
          use(href='../assets/svg/icon-sprite.svg#stroke-animation')
    form.col-sm-4(action='#', method='get').form-inline.search-full.d-none.d-xl-block
      .form-group
        .Typeahead.Typeahead--twitterUsers
          .u-posRelative
            input.demo-input.Typeahead-input.form-control-plaintext.w-100(type='text' placeholder='Type to Search ..' name='q' title='' autofocus)
            svg.search-bg.svg-color
              use(href='../assets/svg/icon-sprite.svg#search')
    .nav-right.col-xl-8.col-lg-12.col-auto.pull-right.right-header.p-0
      ul.nav-menus
        li.serchinput
          .serchbox
            svg
              use(href='../assets/svg/icon-sprite.svg#search')
          .form-group.search-form
              input(type="text" placeholder="Search here...")
        li.onhover-dropdown 
          .notification-box
