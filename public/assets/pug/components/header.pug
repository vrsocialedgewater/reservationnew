                li 
                  a.f-w-700(href="letter-box.html") Check all 
        
        li.onhover-dropdown
          svg
            use(href='../assets/svg/icon-sprite.svg#Bookmark')
          .onhover-show-div.bookmark-flip
            .flip-card
              .flip-card-inner
                .front
                  h6.f-18.mb-0.dropdown-title Bookmark
                  ul.bookmark-dropdown
                    li
                      .row
                        .col-4.text-center
                          a(href="form-validation.html")
                            .bookmark-content
                              .bookmark-icon.bg-light-primary
                                i(data-feather='file-text')
                              span Forms
                        .col-4.text-center
                          a(href="user-profile.html")
                            .bookmark-content 
                              .bookmark-icon.bg-light-secondary
                                i(data-feather='user')
                              span Profile
                        .col-4.text-center
                          a(href="bootstrap-basic-table.html")
                            .bookmark-content
                              .bookmark-icon.bg-light-warning 
                                i(data-feather='server') 
                              span Tables 
                    li.text-centermedia-body 
                      a.flip-btn#flip-btn.f-w-700(href="javascript:void(0)") Add New Bookmark
                .back
                  ul
                    li
                      .bookmark-dropdown.flip-back-content
                        input(type='text' placeholder='search...')
                    li
                      a.f-w-700.d-block.flip-back#flip-back(href="javascript:void(0)")
                        | Back
        li.onhover-dropdown 
          .message.position-relative
            svg
              use(href='../assets/svg/icon-sprite.svg#Message')
            span.rounded-pill.badge-danger
          .onhover-show-div.message-dropdown
            h6.f-18.mb-0.dropdown-title Message                               
            ul
              li
                .d-flex.align-items-start
                  .message-img.bg-light-primary
                    img(src="../assets/images/user/3.jpg" alt="")
