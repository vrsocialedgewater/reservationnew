
doctype html
html(lang='en')
  include ../../components/header-files
  body
    include ../../components/loader
    include ../../components/taptop
    // page-wrapper Start
    #pageWrapper.page-wrapper.compact-wrapper
      include ../../components/header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          .container-fluid
            .page-title
              .row
                .col-xl-4.col-sm-7.box-col-3
                  h3
                    | Map JS
                .col-5.d-none.d-xl-block
                  include ../../components/sub-header
                .col-xl-3.col-sm-5.box-col-4
                  ol.breadcrumb
                    li.breadcrumb-item
                      a(href='index.html')
                        svg.stroke-icon
                          use(href='../assets/svg/icon-sprite.svg#stroke-home')
                    li.breadcrumb-item Maps 
                    li.breadcrumb-item.active Map JS
          // Container-fluid starts
          .container-fluid
            .row
              .col-xl-6
                .card
                  .card-header.pb-0
                    h4 Map at a specified location
                    span Display a map at a specified location and zoom level.
                  .card-body.z-1
                    #weathermap.map-js-height
              .col-xl-6
                .card
                  .card-header.pb-0
                    h4 Map using View Bounds
                    span Display a map of a given area.
                  .card-body.z-1
                    #map2.map-js-height 
              
          // Container-fluid Ends
        include ../../components/footer
    include ../../components/footer-files
