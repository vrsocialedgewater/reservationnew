
doctype html
html(lang='en')
  include ../../components/header-files
  body
    include ../../components/loader
    include ../../components/taptop
    // page-wrapper Start 
    #pageWrapper.page-wrapper.compact-wrapper
      include ../../components/header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          .container-fluid
            .page-title
              .row
                .col-xl-4.col-sm-7.box-col-3
                  h3
                    | Vector Maps
                .col-5.d-none.d-xl-block
                  include ../../components/sub-header
                .col-xl-3.col-sm-5.box-col-4
                  ol.breadcrumb
                    li.breadcrumb-item
                      a(href='index.html')
                        svg.stroke-icon
                          use(href='../assets/svg/icon-sprite.svg#stroke-home')
                    li.breadcrumb-item Maps
                    li.breadcrumb-item.active Vector Maps
          // Container-fluid starts
          .container-fluid
            .row
              .col-sm-6
                .card
                  .card-header.pb-0
                    h4 Vector World Map
                    span Below Map is displaying the world map.
                  .card-body
                    #world-map.jvector-map-height
              .col-sm-6
                .card
                  .card-header.pb-0
